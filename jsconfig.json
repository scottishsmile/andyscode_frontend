{
  "compilerOptions": {
      "baseUrl": "./src/",
      "paths": {
        "@/actions/*": ["store/actions/*"],
        "@/auth/*" : ["pages/api/auth/*"],
        "@/blog/*" : ["pages/blog/*"],
        "@/components/*": ["components/*"],
        "@/constants*": ["constants/*"],
        "@/logger/*": ["logger/*"],
        "@/models/*": ["models/*"],
        "@/reducers/*": ["store/reducers/*"],
        "@/shared/*": ["shared/*"],
        "@/slices/*": ["slices/*"],
        "@/store/*": ["store/*"],
        "@/styles/*": ["styles/*"],
        "@/utils/*": ["utils/*"],
      }
    },
    "exclude": [
      "node_modules"
    ]
}